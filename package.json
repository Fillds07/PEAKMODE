{
  "name": "peakmode",
  "version": "1.0.0",
  "description": "PEAKMODE - Energy management and wellness application",
  "main": "index.js",
  "scripts": {
    "frontend": "cd frontend && npm start",
    "backend": "cd backend && npm run dev",
    "start": "concurrently --kill-others --prefix \"[{name}]\" --names \"BACKEND,FRONTEND\" --prefix-colors \"cyan.bold,green.bold\" \"npm run backend\" \"npm run frontend\"",
    "mongo:start": "brew services start mongodb/brew/mongodb-community@7.0",
    "mongo:stop": "brew services stop mongodb/brew/mongodb-community@7.0",
    "install:all": "npm install && npm run install:backend && npm run install:frontend",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install --legacy-peer-deps",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:backend": "./start-backend.sh",
    "start:frontend": "./start-frontend.sh",
    "start:all": "./start-all-in-one.sh"
  },
  "keywords": [
    "peakmode",
    "wellness",
    "fitness",
    "energy"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "~19.0.10",
    "concurrently": "^8.2.2",
    "typescript": "~5.8.3"
  },
  "dependencies": {
    "expo": "^53.0.7"
  }
}
